include $(JMAXDISTDIR)/Makefiles/Makefile.sgi

SYSTEM_CFLAGS := -mips4 -n32 -r10000 -LANG:restrict 
SYSTEM_LDFLAGS :=  -mips4 -n32
SYSTEM_SOFLAGS := -mips4 -n32 -update_registry /dev/null

SYSTEM_OPT_CFLAGS := -O3 -OPT:roundoff=3:IEEE_arithmetic=3:alias=typed:Olimit=0 -TARG:platform=ip27:processor=r10000 -float_const -INLINE:=ON:dfe=ON  -LNO:opt=1 
SYSTEM_OPT_LDFLAGS = -O3  
SYSTEM_OPT_SOFLAGS = 

ARCH_CFLAGS := $(SYSTEM_CFLAGS) -DSGI -DIRIX6_5
ARCH_LDFLAGS := $(SYSTEM_LDFLAGS)
ARCH_SOFLAGS := $(SYSTEM_SOFLAGS)

ARCH_OPT_CFLAGS := $(SYSTEM_OPT_CFLAGS)
ARCH_OPT_LDFLAGS = $(SYSTEM_OPT_LDFLAGS)
ARCH_OPT_SOFLAGS = $(SYSTEM_OPT_SOFLAGS)

ARCH_DEBUG_CFLAGS := -LANG:restrict

